    "amazon-cognito-identity-js": "^3.2.4",
    "aws-sdk": "^2.620.0",
    "core-js": "^2.6.11",
    "hapkido-auth-lib": "^0.1.12",
    "ionic": "^5.4.16",
    "jsonwebtoken": "^8.5.1",
