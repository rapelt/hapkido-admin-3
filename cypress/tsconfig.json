{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "../node_modules",
    "types": [
      "cypress"
    ]
  },
  "include": [
    "**/*.js"
  ],
  "ordered-imports": [
    true,
    {
      "import-sources-order": "any",
      "named-imports-order": "any"
    }
  ],
  "object-literal-sort-keys": false,
  "no-string-literal": false
}
